                    <a href="" class="nav-link"><i data-feather="file-text"></i> <span>Reports</span></a>
                    <ul>
                        <li><a href="/">Slot Comparison</a></li>
                        <li><a href="/">Date Range</a></li>
                        <li><a href="/">Programme</a></li>
                        <li><a href="/">Advert</a></li>
                        <li><a href="/">Product</a></li>
                        <li><a href="/">Item</a></li>
                    </ul>
                </li>

                <li class="nav-item with-sub">
                    <a href="" class="nav-link"><i data-feather="edit"></i> <span>Custom</span></a>
                    <ul>
                        <li><a href="/">Resend Report</a></li>
                        <li><a href="/">Custom Metrics</a></li>
                    </ul>
                </li>


                <li class="nav-label mg-t-25">Social</li>

                <li class="nav-item with-sub">
                    <a href="" class="nav-link"><i data-feather="play-circle"></i> <span>Social Media</span></a>
                    <ul>
                        <li><a href="/">Added Videos</a></li>
                    </ul>
                </li>
                <li class="nav-item with-sub">
                    <a href="" class="nav-link"><i data-feather="bar-chart-2"></i> <span>Ads Tracker</span></a>
                    <ul>
                        <li><a href="/">Advert Statistics</a></li>
                        <li><a href="/">Product Statistics</a></li>
                    </ul>
                </li>

                <li class="nav-item with-sub">
                    <a href="" class="nav-link"><i data-feather="trending-up"></i> <span>Survey</span></a>
                    <ul>
                        <li><a href="/"> Active Surveys</a></li>
                        <li><a href="/">Survey Emulator</a></li>
                    </ul>
                </li>
            </ul>
        </div>
    </aside>
    <div class="content ht-100v pd-0">
        <div class="content-header">
            <div class="content-search">
                <i data-feather="search"></i>
                <input type="search" class="form-control" placeholder="Search...">
            </div>
            <nav class="nav">
                <a href="" class="nav-link"><i data-feather="help-circle"></i></a>
                <a href="" class="nav-link"><i data-feather="grid"></i></a>
                <a href="" class="nav-link"><i data-feather="align-left"></i></a>
            </nav>
        </div><!-- content-header -->

        <div class="content-body">

            <!--page content-->
            @RenderBody()
            <!--End of page content-->

        </div>
        <footer class="footer tva-only-display">
            <div>
                <div class="float-left">
                    Copyright &copy; Crudsoft Technologies
                    <script>
                        document.write(new Date().getFullYear())
                    </script>
                </div>
                <div class="float-right">
                    <a href="mailto:support@crudsofttechnologies.com"> Help </a>
                </div>
            </div>
        </footer>
    </div>

    <script src="/lib/jquery/jquery.min.js"></script>
    <script src="/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="/lib/feather-icons/feather.min.js"></script>
    <script src="/lib/perfect-scrollbar/perfect-scrollbar.min.js"></script>
    <script src="/lib/chart.js/Chart.bundle.min.js"></script>
    <script src="/lib/jquery.flot/jquery.flot.js"></script>
    <script src="/lib/jquery.flot/jquery.flot.stack.js"></script>
    <script src="/lib/jquery.flot/jquery.flot.resize.js"></script>

    <script src="/lib/formantic/semantic.min.js"></script>

    <script src="/assets/js/dashforge.js"></script>
    <script src="/assets/js/dashforge.aside.js"></script>
    <script src="/assets/js/dashforge.sampledata.js"></script>
    <script src="/assets/js/material-design-kit.js" type="text/javascript"></script>



    @RenderSection("Scripts", required: false)


