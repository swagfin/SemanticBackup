                        <li class="nav-item"><a href="" class="nav-link"><i data-feather="settings"></i> <span>Account Settings</span></a></li>
                        <li class="nav-item"><a href="/User/SignOut" class="nav-link"><i data-feather="log-out"></i> <span>Sign Out</span></a></li>
                    </ul>
                </div>
            </div><!-- aside-loggedin -->
            <ul class="nav nav-aside">
                <li class="nav-label">Dashboards</li>
                <li class="nav-item active"><a href="/Dashboards/Dashboard" class="nav-link"><i data-feather="globe"></i> <span>Dashboard</span></a></li>
                <li class="nav-item"><a href="/" class="nav-link"><i data-feather="globe"></i> <span>Live Dashboard</span></a></li>
                <li class="nav-item"><a href="/" class="nav-link"><i data-feather="map-pin"></i> <span>Viewers Map</span></a></li>
                <li class="nav-item"><a href="/" class="nav-link"><i data-feather="eye"></i> <span>Audience Views</span></a></li>
                <li class="nav-item"><a href="/" class="nav-link"><i data-feather="eye"></i> <span>Viewer Statistics</span></a></li>

                <li class="nav-label mg-t-25">System</li>
                <li class="nav-item with-sub">
                    <a href="" class="nav-link"><i data-feather="user"></i> <span>System Users</span></a>
                    <ul>
                        <li><a href="/User/Index">Manage Users</a></li>
                        <li><a href="/">Valid Domains</a></li>
                    </ul>
                </li>
                <li class="nav-item with-sub">
                    <a href="" class="nav-link"><i data-feather="settings"></i> <span>Settings</span></a>
                    <ul>
                        <li><a href="/">Services</a></li>
                        <li><a href="/">Groups</a></li>
                        <li><a href="/">Ads Agents</a></li>
                        <li><a href="/">Networks</a></li>
                        <li><a href="/">Transmitters</a></li>
                        <li><a href="/">Transmitters Maps</a></li>
                        <li><a href="/">Transports</a></li>
                        <li><a href="/">Postcodes</a></li>
                    </ul>
                </li>


                <li class="nav-label mg-t-25">Reporting</li>
