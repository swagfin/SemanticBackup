<!DOCTYPE html>
<html lang="en">
<head>

    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Meta -->
    <meta name="description" content="SEMANTIC BACKUP | BACKUP CLIENT">
    <meta name="author" content="CRUDSOFT TECHNOLOGIES">

    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="~/assets/img/favicon.png">

    <title>@ViewData["Title"]</title>

    <!-- vendor css -->
    <link href="~/lib/fortawesome/fontawesome-free/css/all.min.css" rel="stylesheet">
    <link href="~/lib/ionicons/css/ionicons.min.css" rel="stylesheet">

    <!-- Formantic UI -->
    <link rel="stylesheet" href="~/lib/formantic/semantic.min.css">



    <!-- DashForge CSS -->
    <link rel="stylesheet" href="~/assets/css/dashforge.css">
    <link rel="stylesheet" href="~/assets/css/dashforge.dashboard.css">

    @RenderSection("Styles", required: false)

</head>
<body>

    <aside class="aside aside-fixed">
        <div class="aside-header">
            <div href="~" class="tva-logo">Semantic Backup</div>
            <a href="" class="aside-menu-link">
                <i data-feather="menu"></i>
                <i data-feather="x"></i>
            </a>
        </div>
        <div class="aside-body">
            <div class="aside-loggedin">
                <div class="d-flex align-items-center justify-content-start">
                    <a class="avatar" asp-page="/User/Profile"><img src="~/images/uploads/user_001.jpg" class="rounded-circle" alt=""></a>
                    <div class="aside-alert-link">
                        <a href="" class="new" data-toggle="tooltip" title="You have 4 new notifications"><i data-feather="bell"></i></a>
                        <a href="/User/SignOut" class="nav-link" data-toggle="tooltip" title="Sign out"><i data-feather="log-out"></i></a>
                    </div>
                </div>
                <div class="aside-loggedin-user">
                    <a href="#loggedinMenu" class="d-flex align-items-center justify-content-between mg-b-2" data-toggle="collapse">
                        <h6 class="tx-semibold mg-b-0">George Wainaina</h6>
                        <i data-feather="chevron-down"></i>
                    </a>
                    <p class="tx-color-03 tx-12 mg-b-0">Administrator</p>
                </div>
                <div class="collapse" id="loggedinMenu">
                    <ul class="nav nav-aside mg-b-0">
                        <li class="nav-item"><a href="/User/EditProfile" class="nav-link"><i data-feather="edit"></i> <span>Edit Profile</span></a></li>
                        <li class="nav-item"><a href="/User/Profile" class="nav-link"><i data-feather="user"></i> <span>View Profile</span></a></li>
                        <li class="nav-item"><a href="" class="nav-link"><i data-feather="settings"></i> <span>Account Settings</span></a></li>
                        <li class="nav-item"><a href="/User/SignOut" class="nav-link"><i data-feather="log-out"></i> <span>Sign Out</span></a></li>
                    </ul>
                </div>
            </div><!-- aside-loggedin -->
            <ul class="nav nav-aside">
                <li class="nav-label">Dashboards</li>
                <li class="nav-item active"><a href="~/" class="nav-link"><i data-feather="globe"></i> <span>Dashboard</span></a></li>

                <li class="nav-label mg-t-25">Backup Menu</li>

                 <li class="nav-item"><a href="~/" class="nav-link"><i data-feather="database"></i> <span>Databases</span></a></li>
                 <li class="nav-item"><a href="~/" class="nav-link"><i data-feather="upload-cloud"></i> <span>Database Backups</span></a></li>
                 <li class="nav-item"><a href="~/" class="nav-link"><i data-feather="clock"></i> <span>Backup Schedules</span></a></li>

              
                <li class="nav-label mg-t-25">Custom</li>

                <li class="nav-item with-sub">
                    <a href="" class="nav-link"><i data-feather="edit"></i> <span>Custom Functions</span></a>
                    <ul>
                        <li><a href="/">Requeue</a></li>
                        <li><a href="/">Get Download Link</a></li>
                    </ul>
                </li>



            </ul>
        </div>
    </aside>
    <div class="content ht-100v pd-0">
        <div class="content-header">
            <div class="content-search">
                <i data-feather="search"></i>
                <input type="search" class="form-control" placeholder="Search...">
            </div>
            <nav class="nav">
                <a href="" class="nav-link"><i data-feather="help-circle"></i></a>
                <a href="" class="nav-link"><i data-feather="grid"></i></a>
                <a href="" class="nav-link"><i data-feather="align-left"></i></a>
            </nav>
        </div><!-- content-header -->

        <div class="content-body">

            <!--page content-->
            @RenderBody()
            <!--End of page content-->

        </div>
        <footer class="footer tva-only-display">
            <div>
                <div class="float-left">
                    Copyright &copy; Crudsoft Technologies
                    <script>
                        document.write(new Date().getFullYear())
                    </script>
                </div>
                <div class="float-right">
                    <a href="mailto:support@crudsofttechnologies.com"> Help </a>
                </div>
            </div>
        </footer>
    </div>

    <script src="/lib/jquery/jquery.min.js"></script>
    <script src="/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="/lib/feather-icons/feather.min.js"></script>
    <script src="/lib/perfect-scrollbar/perfect-scrollbar.min.js"></script>
    <script src="/lib/chart.js/Chart.bundle.min.js"></script>
    <script src="/lib/jquery.flot/jquery.flot.js"></script>
    <script src="/lib/jquery.flot/jquery.flot.stack.js"></script>
    <script src="/lib/jquery.flot/jquery.flot.resize.js"></script>

    <script src="/lib/formantic/semantic.min.js"></script>

    <script src="/assets/js/dashforge.js"></script>
    <script src="/assets/js/dashforge.aside.js"></script>
    <script src="/assets/js/dashforge.sampledata.js"></script>
    <script src="/assets/js/material-design-kit.js" type="text/javascript"></script>



    @RenderSection("Scripts", required: false)


</body>
</html>
