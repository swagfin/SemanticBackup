{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "LiteDbPersistanceOptions": {
    "ConnectionString": "{{env}}/data/database.secured.db"
  },
  "PersistanceOptions": {
    "ServerDefaultTimeZone": "Africa/Nairobi",
    "DefaultBackupExpiryAgeInDays": 14,
    "MaximumBackupRunningThreads": 10,
    "DefaultBackupDirectory": "/",
    "EnsureDefaultBackupDirectoryExists": false,
    "BackupFileSaveFormat": "{{database}}-{{datetime}}.{{databasetype}}.bak",
    "CompressBackupFiles": false
  }
}
