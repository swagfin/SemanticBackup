{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "LiteDbPersistanceOptions": {
    "ConnectionString": "{{env}}\\data\\database.secured.db"
  },
  "PersistanceOptions": {
    "ServerDefaultTimeZone": "E. Africa Standard Time",
    "DefaultBackupExpiryAgeInDays": 14,
    "MaximumBackupRunningThreads": 10,
    "DefaultBackupDirectory": "c:\\backups\\"
  }
}
